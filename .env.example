# API Keys for LLM Providers
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here

# LLM Provider Selection (openai, anthropic)
LLM_PROVIDER=openai

# Model Configuration for OpenAI
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Model Configuration for Anthropic
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Embedding Provider Selection (openai, sentence_transformers)
EMBEDDING_PROVIDER=openai

# Model Configuration for Sentence Transformers
HF_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Optional: Database Path (default is in-memory)
DATABASE_PATH=/app/data/pydantic_rag.db

# Optional: Cache Settings
ENABLE_CACHE=true
CACHE_TTL=3600